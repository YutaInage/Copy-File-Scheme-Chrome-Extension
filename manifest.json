{
  "name": "Copy File Scheme",
  "version": "0.1",
  "description": "copy file scheme to clipbord.",
  "background_page": "background.html",
  "browser_action": {
    "default_title": "Copy File Scheme"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["copyFilePath.js"]
    }
  ],
  "permission": [
    "clipboardWrite"
  ]
}
